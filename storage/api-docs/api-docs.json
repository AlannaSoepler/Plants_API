  "paths": {
    "/api/plants": {
      "get": {
        "tags": ["Plants"],
        "summary": "Get all Plants",
        "description": "Displays all the plants",
        "operationId": "6e2a3a91967b1e364c87b755cd5152f6",
        "responses": {
          "200": {
            "description": "Successful operation, Returns a list of Plants in JSON format"
          },
          "401": {
            "description": "Unauthenticated"
          },
          "403": {
            "description": "Forbidden"
          }
        }
      },
      "post": {
        "tags": ["Plants"],
        "summary": "Create a new Plant",
        "description": "Stores the plant in the DB",
        "operationId": "store",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "name",
                  "breed",
                  "image",
                  "info",
                  "season",
                  "hight",
                  "likes",
                  "provider_id"
                ],
                "properties": {
                  "name": {
                    "type": "string",
                    "format": "string",
                    "example": "hibiscus"
                  },
                  "breed": {
                    "type": "string",
                    "format": "string",
                    "example": "malvaceae"
                  },
                  "image": {
                    "type": "string",
                    "format": "string",
                    "example": "https://xyz.com"
                  },
                  "info": {
                    "type": "string",
                    "format": "string",
                    "example": "the hibiscus is a wonderful plant"
                  },
                  "season": {
                    "type": "string",
                    "default": "summer",
                    "enum": ["summer", "fall", "winter", "spring"]
                  },
                  "hight": {
                    "type": "float",
                    "format": "float",
                    "example": "2.5"
                  },
                  "likes": {
                    "type": "integer",
                    "format": "integer",
                    "example": "1592"
                  },
                  "provider_id": {
                    "type": "number",
                    "example": "1"
                  }
                },
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": ""
                    },
                    "data": {
                      "type": "object"
                    }
                  },
                  "type": "object"
                }
              }
            }
          }
        },
        "security": [{ "bearerAuth": [] }]
      }
    },
